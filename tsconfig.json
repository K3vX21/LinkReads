{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/providers.jsx",
    "app/page.jsx",
    "middleware.js",
    "utils/supabaseRouteHandler.js",
    "app/auth/callback/route.js",
    "app/auth/sign-in/route.js",
    "app/auth/sign-out/route.js",
    "app/auth/sign-up/route.js",
    "app/_login/page.jsx"
, "app/explore/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}